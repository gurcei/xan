#OUTPUT "BIGSPRITE"#DECLARE A$ = ""#DECLARE IDX = 0#DECLARE спрь64ен = $D057#DECLARE спрхгтен = $D055#DECLARE спрхгт   = $D056#DECLARE спрптр16 = $D06E#DECLARE спрптрадр = $D06C#DECLARE спрптрбнк = $D06E' LET ALL SPRITES BE 64 BITS WIDEPOKE спрь64ен, 255' LET ALL SPRITES BE 64 BITS HIGHPOKE спрхгтен, 255POKE спрхгт, 64' ENFORCE SPRITES INTO банк4 AND спрптр16 BIT TURNED ONPOKE спрптрбнк, 4+128           ' WPOKE спрптрадр, 0^^WPOKE $D06C, 0' LOAD 64X64 SPRITE DATA^^BLOAD "MUSE.DAT",P($40040),R      ' DISPLAY SPRITESPRITE 0,1,0MOVSPR 0,0+18,60SPRITE 1,1,0MOVSPR 1,85+18,60SPRITE 2,1,0MOVSPR 2,171+18,60SPRITE 3,1,0MOVSPR 3,256+18,60SPRITE 4,1,0MOVSPR 4,0+18+8,160' POINT SPRITE0 TO $4040BANK 4^^WPOKE 0, $1001^^WPOKE 2, $1001+8*6^^WPOKE 4, $1001+8*12^^WPOKE 6, $1001+8*18^^WPOKE 8, $1001+8*24' SET BACKGROUND, BORDERBACKGROUND 6BORDER 14.LOOP' SELECT SPRITE FRAMEBANK 4POKE 0, 1+8*IDXPOKE 2, 1+8*(IDX+6)POKE 4, 1+8*(IDX+12)POKE 6, 1+8*(IDX+18)POKE 8, 1+8*(IDX+24)SLEEP .3IDX=IDX+1IF IDX=6 THEN IDX=0GET A$:IF A$="" THEN GOTO LOOPSPRITE 0,0SPRITE 1,0SPRITE 2,0